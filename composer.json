{
    "name": "mmi/mmi",
    "type": "library",
    "description": "MMi Framework Library",
    "keywords": ["mmi", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mariusz Miłejko",
            "email": "mariusz@milejko.pl"
        },
        {
            "name": "Bartłomiej Wołos",
            "email": "funfel@onet.pl"
        },
        {
            "name": "Maciej Pawłowski",
            "email": "mac@macu.eu"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-bcmath": "*",
        "ext-date": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-ldap": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-spl": "*",
        "monolog/monolog": "1.*",
        "graylog2/gelf-php": "1.5.*",
        "php-di/php-di": "^6.3",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "doctrine/annotations": "^1.11"
    },
    "require-dev": {
        "php-console/php-console": "~3.1",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Mmi\\": "src/Mmi",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmi\\Tests\\": "tests/Mmi"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
